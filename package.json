{
  "name": "zc_plugin_company_goals",
  "version": "1.0.0",
  "main": "server.js",
  "repository": "git@github.com:zurichat/zc_plugin_company_goals.git",
  "type": "commonjs",
  "scripts": {
    "client:build": "cross-env INLINE_RUNTIME_CHUNK=false yarn --cwd client run build ",
    "client:dev": "yarn --cwd client start",
    "dev": "concurrently --kill-others-on-fail \"yarn run server\" \"yarn run client:dev\"",
    "server": "nodemon server.js",
    "server:prod": "cross-env NODE_ENV=production nodemon server.js",
    "start": "node server.js",
    "prepare": "husky install",
    "format": "prettier -w .",
    "lint": "eslint "
  },
  "author": "Team Plato",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.4",
    "centrifuge": "^2.8.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.3.0",
    "helmet": "^4.6.0",
    "joi": "^17.4.2",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^8.3.2",
    "uuidv4": "^6.2.12",
    "winston": "^3.3.3",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "cspell": "^5.8.2",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.25.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --cache --fix"
    ]
  },
  "engines": {
    "node": "^14"
  }
}
